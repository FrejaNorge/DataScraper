            					var fromMonth = parseInt(vonD.slice(3,5));
            					
            					var toDay = parseInt(bisD.slice(0,2));
            					var toMonth = parseInt(bisD.slice(3,5));
            					
            					if((fromDay > 31 || fromDay == 0 ) || (fromMonth > 12 || fromMonth == 0) || 
