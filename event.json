            						return false;
            					}
            					
            					matchesFrom = /^(\d{2})[.\/](\d{2})[.\/](\d{4})$/.exec(vonD);
            					matchesTo = /^(\d{2})[.\/](\d{2})[.\/](\d{4})$/.exec(bisD);
            				} else if (bisD != "") {
            					matchesTo = /^(\d{2})[.\/](\d{2})[.\/](\d{4})$/.exec(bisD);
            				} else if (vonD != "") {
            					matchesFrom = /^(\d{2})[.\/](\d{2})[.\/](\d{4})$/.exec(vonD);
            				}
            
            				if (matchesTo === null || matchesFrom === null || matchesTo === 1 || matchesFrom === 1) {
            					window.alert(error_msgAll);
            					return false;
            				}
            
            				var fromDate = new Date(matchesFrom[3], matchesFrom[2] -1, matchesFrom[1]);
    						var toDate = new Date(matchesTo[3], matchesTo[2] -1, matchesTo[1]);
							
