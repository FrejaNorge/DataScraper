                            }
                        });

                        jQuery("#fbLayer-connected, #fbLayerLogin-connected, #fbLayerLoginLink-connected").show();
                        jQuery("#fbIframeButton,#fbIframeButtonLogin").hide().remove();
                    } else if (response.status === 'not_authorized'){
                        window.location = deleteURL;
