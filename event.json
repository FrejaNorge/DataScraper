            ( function() {
            
            		jQuery('.tpd-content .testFormRange').each(function() {

                        var tmpFunc = function(date, el) {
                            var tmpInput = el.input[0] || false;                            
                            if(!tmpInput) return;

                            jQuery(tmpInput).siblings(".input").val(date).addClass('set');
                            jQuery(this).hide();
