        jQuery('#fb-disconnect').click(function(e) {
            e.preventDefault();
            var deleteURL = jQuery(this).attr("href");
            jQuery('#facebookDisconnectError').hide();
            FB.getLoginStatus(function(response) {
                try {
                    container = document.getElementById('iLoginContainer');
                    if (container == null)
                        return;

                    container.innerHTML = "";
                    if (response.status === 'connected') {
                        var uid = response.authResponse.userID;
                        var accessToken = response.authResponse.accessToken;
                        FB.XFBML.parse(container);

                        var ajaxURL = "https://graph.facebook.com/" + uid + "/permissions";

                        jQuery.ajax({
                            url: ajaxURL,
                            type: 'POST',
                            cache: false,
                            data: "method=delete&access_token=" + accessToken,
                            dataType: 'jsonp',
                            success: function (data) {
