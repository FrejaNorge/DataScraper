                            });      			                			   
            			});
            			            			            
            			jQuery(this).on('submit', function(e) {
            
            				//Vorab prüfen, ob das Datum dem vorgeschriebenen Format entspricht
            				var vonD = jQuery("#von").val();
            				var bisD = jQuery("#bis").val();
							
            				var error_msg = "Indtast venligst en gyldig dato, f.eks.";
            				var error_msg2 = ".";
            
            				var d1 = "22.04.2020";
            				var error_msgAll = error_msg + d1 + error_msg2;
            				var matchesFrom = 1;
            				var matchesTo = 1;
            
            				if (vonD != "" && bisD != "") {
            					var fromDay	= parseInt(vonD.slice(0,2));
            					var fromMonth = parseInt(vonD.slice(3,5));
            					
            					var toDay = parseInt(bisD.slice(0,2));
            					var toMonth = parseInt(bisD.slice(3,5));
            					
            					if((fromDay > 31 || fromDay == 0 ) || (fromMonth > 12 || fromMonth == 0) || 
            					   (toDay > 31 || toDay == 0 ) || (toMonth > 12 || toMonth == 0)){
