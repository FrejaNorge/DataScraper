<script type="text/javascript">
    (function($) {

        "use strict";

        function fixLayerAtFooter() {
            var footer = document.getElementById('newfooter');
            var cookieHintContainer = document.getElementById('cookie-hint-container');
            var cookieHint = document.getElementById('cookie-hint');
            if (footer.getBoundingClientRect().top < window.innerHeight) {
                cookieHintContainer.className = 'scroll';
                cookieHint.style.bottom = (footer.getBoundingClientRect().height - 50) + 'px';
                if (window.innerWidth >= 769) {
                    cookieHint.style.left = (20 - footer.getBoundingClientRect().left) + 'px';
                }
            } else {
                cookieHintContainer.className = '';
                cookieHint.style.bottom = '';
                cookieHint.style.left = '';
            }
        }

        var CookieHint = {

            init: function(){
    			this.cacheDom();
                this.showLayer();
                this.bindEvents();
    			this.hideLayer();
    		},

    		cacheDom: function(){
    			this.cookieLayer = $("#cookie-hint");
    			this.cookieButton = $(".cookie-hint-button");
    		},

            showLayer: function() {
                if(localStorage.getItem("cookiehint") === null) {
                    this.cookieLayer.show();
                }
            },

    		bindEvents: function(){
    			this.cookieButton.click(function(){
    				CookieHint.storeInLocalStorage();
    				CookieHint.hideLayer(true);
    			});
                if(!localStorage.getItem("cookiehint")) {
                    $(window).scroll(fixLayerAtFooter);
                }
                
    		},

    		hideLayer: function(arg) {
                if(localStorage.getItem("cookiehint")) {
                    arg ? this.cookieLayer.fadeOut('fast') : this.cookieLayer.hide();
                }
    		},

    		storeInLocalStorage: function(){
    			localStorage.setItem("cookiehint", 1);
    		}
    	};

        $(function() {
            CookieHint.init();
        });
}(jQuery));

</script>


