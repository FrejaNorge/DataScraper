            				});
            			};
            
            			var calculatePeriodRange = function(input) {
            
            				var now = new Date();
            
            				var fromEl = jQuery("#von").get(0) || false;
            				var fromValue = fromEl.value || '';
							matchesFromVal = /^(\d{2})[.\/](\d{2})[.\/](\d{4})$/.exec(fromValue);
            
            				var toEl = jQuery("#bis").get(0) || false;
            				var toValue = toEl.value || '';
							matchesToVal = /^(\d{2})[.\/](\d{2})[.\/](\d{4})$/.exec(toValue);
            											
							if (input.id == 'von') {
								if(matchesToVal !== null && matchesFromVal === null){
                                	minDate = new Date(matchesToVal[3], matchesToVal[2] - 1, matchesToVal[1]);
                                	jQuery('#bisDatepicker').datepicker({ dateFormat: 'yyyy-mm-dd' });
    								jQuery('#bisDatepicker').datepicker('setDate', minDate);
								} else if(matchesFromVal !== null) {
									minDate = new Date(matchesFromVal[3], matchesFromVal[2] - 1, matchesFromVal[1]);
                                	jQuery('#bisDatepicker').datepicker({ dateFormat: 'yyyy-mm-dd' });
    								jQuery('#bisDatepicker').datepicker('setDate', minDate);
								}
            				} else {
								if(matchesFromVal !== null && matchesToVal === null){
                                	minDate = new Date(matchesFromVal[3], matchesFromVal[2] - 1, matchesFromVal[1]);
                                	jQuery('#bisDatepicker').datepicker({ dateFormat: 'yyyy-mm-dd' });
    								jQuery('#bisDatepicker').datepicker('setDate', minDate);
								} else if(matchesToVal !== null) {
									minDate = new Date(matchesToVal[3], matchesToVal[2] - 1, matchesToVal[1]);
                                	jQuery('#bisDatepicker').datepicker({ dateFormat: 'yyyy-mm-dd' });
    								jQuery('#bisDatepicker').datepicker('setDate', minDate);
								}
            				}
            			};
            
                        //initialize datepickers
            			jQuery('#vonDatepicker, #bisDatepicker').each(function() {            
            				initializePeriodDatepicker(this);
            				jQuery(this).click(function(event) { event.stopPropagation(); });
							Tipped.refresh(jQuery('#filterZei')[0]);
            			});
            			            			
            			//functionality for input fields
            			jQuery('#von, #bis').each(function() {
                            
                            var inputField = jQuery(this);
                                                                          			      
            			    jQuery(this).focusin(function(event) {
            			        
            			        Tipped.refresh(jQuery('#filterZei')[0]);
            			        if(inputField.attr("id")=="von"){
            			            jQuery("#bisDatepicker").hide();   
            			        }else{            			            
            			            jQuery("#vonDatepicker").hide();    
